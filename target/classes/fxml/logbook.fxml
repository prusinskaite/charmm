<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox  prefWidth="1000" prefHeight="700" styleClass="root" stylesheets="/css/login.css" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.LogbookController">
    <children>
        <Button fx:id="back" onAction="#goBack" styleClass="back" text="%login.back" />
        <Label fx:id="patientLabel" layoutX="20.0" layoutY="41.0" styleClass="bigLabel" />
        <ScrollPane fitToHeight="true" fitToWidth="true" styleClass="root">
            <VBox fx:id="sliderContainer" styleClass="sliderContainer">
                <opaqueInsets>
                    <Insets />
                </opaqueInsets>
                <padding>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                </padding>
            </VBox>

        </ScrollPane>
    </children>
</VBox>
